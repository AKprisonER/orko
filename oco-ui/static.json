{
  "root": "build/",
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=7776000"
    }
  },
  "debug": true,
  "proxies": {
    "/ws/": {
      "origin": "${API_URL}/ws",
      "ws": true
    },
    "/api/": {
      "origin": "${API_URL}"
    }
  },
  "routes": {
    "/coin/**": "index.html",
    "/job/**": "index.html"
  }
}