web: java $JAVA_OPTS -Ddw.server.connector.port=$PORT -cp orko-web/target/classes:orko-web/target/dependency/* com.grahamcrockford.orko.web.WebApplication server heroku-config.yml
worker: java $JAVA_OPTS -Ddw.server.connector.port=$PORT -cp orko-worker/target/classes:orko-worker/target/dependency/* com.grahamcrockford.orko.worker.WorkerApplication server heroku-config.yml