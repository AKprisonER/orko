web: java $JAVA_OPTS -Ddw.server.connector.port=$PORT -cp oco-web/target/classes:oco-web/target/dependency/* com.grahamcrockford.oco.web.WebApplication server example-config.yml
worker: java $JAVA_OPTS -Ddw.server.connector.port=$PORT -cp oco-worker/target/classes:oco-worker/target/dependency/* com.grahamcrockford.oco.worker.WorkerApplication server example-config.yml